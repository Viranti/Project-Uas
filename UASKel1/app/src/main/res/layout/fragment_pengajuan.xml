<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingHorizontal="32dp"
    android:paddingVertical="32dp"
    android:background="@color/primary"
    tools:context=".PengajuanFragment">

    <!-- TODO: Update blank fragment layout -->
    <ImageView
        android:id="@+id/ic_back"
        android:layout_width="32dp"
        android:layout_height="32dp"
        android:src="@drawable/ic_back"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />
    <com.google.android.material.button.MaterialButton
        android:id="@+id/btn_tambahpengajuan"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:strokeWidth="2dp"
        app:strokeColor="@color/black"
        android:backgroundTint="@color/primary"
        android:text="@string/tambahpengajuan"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textAlignment="textStart"
        app:icon="@drawable/ic_plus"
        app:iconSize="32dp"
        app:iconTint="@color/black"
        app:iconGravity="end"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/ic_back"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <TextView
        android:id="@+id/tx1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pengajuan Terkirim"
        android:textSize="18sp"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/btn_tambahpengajuan"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <LinearLayout
        android:id="@+id/list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        app:layout_constraintTop_toBottomOf="@+id/tx1"
        app:layout_constraintLeft_toRightOf="parent"
        android:orientation="vertical">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/card1"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:layout_marginBottom="16dp"
            android:background="@drawable/rounded_nonstroke"
            android:padding="10dp"
            android:backgroundTint="@color/white"
            >
            <ImageView
                android:id="@+id/img_pengajuan"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_marginTop="12dp"
                android:layout_marginLeft="20dp"
                android:src="@drawable/jalan"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                />

            <TextView
                android:id="@+id/jdl_pengajuan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/jdlpengajuan1"
                android:textColor="@color/black"
                android:layout_marginTop="12dp"
                android:textSize="16sp"
                android:textStyle="bold"
                android:layout_marginLeft="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/img_pengajuan"
                />

            <TextView
                android:id="@+id/tgl_pengajuan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tglpengajuan"
                android:textColor="@color/black"
                android:textSize="10sp"
                android:layout_marginLeft="10dp"
                app:layout_constraintLeft_toRightOf="@id/img_pengajuan"
                app:layout_constraintTop_toBottomOf="@+id/jdl_pengajuan"
                />

            <TextView
                android:id="@+id/desc_pengajuan"
                android:layout_marginTop="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/descpengajuan1"
                android:textColor="@color/black"
                android:textSize="10sp"
                android:layout_marginLeft="20dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/img_pengajuan"
                />

            <ImageView
                android:id="@+id/ic_edit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:src="@drawable/ic_edit"
                android:layout_marginRight="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toLeftOf="@+id/ic_hapus"
                />
            <ImageView
                android:id="@+id/ic_hapus"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:src="@drawable/ic_hapus"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/card2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="16dp"
            android:background="@drawable/rounded_nonstroke"
            android:backgroundTint="@color/white"
            android:padding="10dp"
            >
            <ImageView
                android:id="@+id/img_pengajuan2"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_marginTop="12dp"
                android:layout_marginLeft="20dp"
                android:src="@drawable/rumah"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                />

            <TextView
                android:id="@+id/jdl_pengajuan2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/jdlpengajuan2"
                android:textColor="@color/black"
                android:layout_marginTop="12dp"
                android:textSize="16sp"
                android:textStyle="bold"
                android:layout_marginLeft="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/img_pengajuan2"
                />

            <TextView
                android:id="@+id/tgl_pengajuan2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tglpengajuan"
                android:textColor="@color/black"
                android:textSize="10sp"
                android:layout_marginLeft="10dp"
                app:layout_constraintLeft_toRightOf="@id/img_pengajuan2"
                app:layout_constraintTop_toBottomOf="@+id/jdl_pengajuan2"
                />

            <TextView
                android:id="@+id/desc_pengajuan2"
                android:layout_marginTop="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/descpengajuan2"
                android:textColor="@color/black"
                android:textSize="10sp"
                android:layout_marginLeft="20dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/img_pengajuan2"
                />

            <ImageView
                android:id="@+id/ic_edit2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:src="@drawable/ic_edit"
                android:layout_marginRight="8dp"
                app:layout_constraintTop_toBottomOf="@+id/desc_pengajuan2"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toLeftOf="@+id/ic_hapus2"
                />
            <ImageView
                android:id="@+id/ic_hapus2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:src="@drawable/ic_hapus"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/card3"
            android:layout_width="match_parent"
            android:layout_height="130dp"
            android:layout_marginBottom="16dp"
            android:background="@drawable/rounded_nonstroke"
            android:backgroundTint="@color/white"
            android:padding="10dp"
            >
            <ImageView
                android:id="@+id/img_pengajuan3"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_marginTop="12dp"
                android:layout_marginLeft="20dp"
                android:src="@drawable/pkh"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                />

            <TextView
                android:id="@+id/jdl_pengajuan3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/jdlpengajuan3"
                android:textColor="@color/black"
                android:layout_marginTop="12dp"
                android:textSize="16sp"
                android:textStyle="bold"
                android:layout_marginLeft="10dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toRightOf="@id/img_pengajuan3"
                />

            <TextView
                android:id="@+id/tgl_pengajuan3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tglpengajuan"
                android:textColor="@color/black"
                android:textSize="10sp"
                android:layout_marginLeft="10dp"
                app:layout_constraintLeft_toRightOf="@id/img_pengajuan3"
                app:layout_constraintTop_toBottomOf="@+id/jdl_pengajuan3"
                />

            <TextView
                android:id="@+id/desc_pengajuan3"
                android:layout_marginTop="8dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/descpengajuan3"
                android:textColor="@color/black"
                android:textSize="10sp"
                android:layout_marginLeft="20dp"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/img_pengajuan3"
                />

            <ImageView
                android:id="@+id/ic_edit3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:src="@drawable/ic_edit"
                android:layout_marginRight="8dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/desc_pengajuan3"
                app:layout_constraintRight_toLeftOf="@+id/ic_hapus3"
                />
            <ImageView
                android:id="@+id/ic_hapus3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:src="@drawable/ic_hapus"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>